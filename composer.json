{
    "name": "pharaonic/laravel-uploader",
    "description": "Laravel Uploader to upload files and handling visibility.",
    "keywords": [
        "laravel uploader",
        "laravel uploads",
        "larave files",
        "uploader",
        "laravel",
        "secure",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Moamen Eltouny (Raggi)",
            "email": "raggigroup@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=9.0",
        "pharaonic/laravel-readable": ">=1.0",
        "intervention/image": "^2.6.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Uploader\\UploaderServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Uploader\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
