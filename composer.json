{
    "name": "pharaonic/laravel-uploader",
    "description": "Laravel Uploader to upload files and handling visibility.",
    "keywords": [
        "php",
        "laravel",
        "uploader",
        "files",
        "secure"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Raggi",
            "email": "support@pharaonic.io",
            "homepage": "https://pharaonic.io"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=8.0",
        "pharaonic/laravel-readable": ">=1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Uploader\\UploaderServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Uploader\\": "src"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
